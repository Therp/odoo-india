<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="view_change_qc_approved_date" model="ir.ui.view">
			<field name="name">QC Approved Date Wizard</field>
			<field name="model">change.qcapproved.date</field>
			<field name="arch" type="xml">
				<form string="QC Approved Date" version="7.0">
					<group>
						<h3>
							<group>
								<field name="qc_approved_date" />
							</group>
						</h3>
						<group />
					</group>
					<footer>
						<button name="to_update" string="_Apply" type="object"
							class="oe_highlight" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_change_qc_approved_date" model="ir.actions.act_window">
			<field name="name">Change QC Approved Date</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">change.qcapproved.date</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

	</data>
</openerp>

